<!DOCTYPE html>
<html lang="es">
<head>
  <%- include ('../partials/headView.ejs') %>
</head>
<body>
  <%- include ('../layouts/headerView.ejs') %>
  <main>
     <div class="main">
       <div class="main__content container">
        <div class="boletin">
          <h1 class="boletin__title"><%= boletin.Titulo %></h1>
          <div class="boletin__content">
            <img src="<%= boletin.Portada %>" alt="<%= `Boletin ${boletin.Titulo}` %>">
            <div>
              <%- boletin.Contenido %>
            </div>
            <% if(boletin.Boletin){ %>
              <h2 class="boletin__download"><a target="_blank" href="<%= boletin.Boletin %>" title="<%= `Archivo ${boletin.Titulo} | Oladeg` %>"  role="link">Descargar Bolet√≠n</a ></h2>
              <div class="boletin__iframe">
                <iframe src="<%= boletin.Boletin %>" frameborder="0" title="<%= `Archivo ${boletin.Titulo} | Oladeg` %>"></iframe>
              </div>
            <%}%>
          </div>
         <%- include('../partials/twobuttons.ejs', {tipo: "boletin"}) %>
        </div>
        <%- include('../layouts/sidebarView.ejs') %>
       </div>
     </div>

    <%- include('../layouts/whatsapp.ejs') %>
  </main> 
  <%- include('../layouts/footerView.ejs') %>
  <%- include('../layouts/scriptView.ejs') %>
</body>
</html>
